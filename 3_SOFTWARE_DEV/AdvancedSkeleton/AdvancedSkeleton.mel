		asUnitConvertAndReverse ("Follow"+$fitJoints[$i]+$side) ("FK"+$fitJoints[$i]+$side+".follow");
		connectAttr ("Follow"+$fitJoints[$i]+$side+"Reverse.outputX") ("FKFollow"+$fitJoints[$i]+$side+"_orientConstraint1.Static"+$fitJoints[$i]+$side+"W0");
		connectAttr ("Follow"+$fitJoints[$i]+$side+"UnitConversion.output") ("FKFollow"+$fitJoints[$i]+$side+"_orientConstraint1.Follow"+$fitJoints[$i]+$side+"W1");

		if ($fitJointFollowTranslate[$i])
			{
			delete ("Follow"+$fitJoints[$i]+$side+"_orientConstraint1");
			parentConstraint -mo ($fitJointsFollow[$i]+$fitJointsFollowSide[$i]) ("Follow"+$fitJoints[$i]+$side);
			pointConstraint ("Static"+$fitJoints[$i]+$side) ("Follow"+$fitJoints[$i]+$side) ("FKFollow"+$fitJoints[$i]+$side);
			connectAttr ("Follow"+$fitJoints[$i]+$side+"Reverse.outputX") ("FKFollow"+$fitJoints[$i]+$side+"_pointConstraint1.Static"+$fitJoints[$i]+$side+"W0");
			connectAttr ("Follow"+$fitJoints[$i]+$side+"UnitConversion.output") ("FKFollow"+$fitJoints[$i]+$side+"_pointConstraint1.Follow"+$fitJoints[$i]+$side+"W1");
			}
		for ($y=0;$y<size($tempString);$y++)
			parent $tempString[$y] ("FKFollow"+$fitJoints[$i]+$side);
		}

//Advanced IkFollow
for ($b=1;$b>-2;$b=$b-2)
