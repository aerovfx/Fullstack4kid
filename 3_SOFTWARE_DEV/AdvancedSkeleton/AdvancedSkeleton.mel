int $reachedStart,$labelNum;
int $startJointLocks[],$endJointLocks[];
float $radius;
float $startJointFat=1;
float $startJointFatFront=1;
float $startJointFatWidth=1;
float $endJointFat=1;
float $endJointFatFront=1;
float $endJointFatWidth=1;
