string $tempString[],$tempString2[];

currentTime 30;
$tempString=`ls -type animCurve "asDSEdit_*"`;
for ($i=0;$i<size($tempString);$i++)
	delete $tempString[$i];

currentTime 0;
$tempString=`ls -type animCurve "asDSDriverEdit_*"`;//driver
for ($i=0;$i<size($tempString);$i++)
	delete $tempString[$i];

currentTime $currentTime;
asDsMuteSDK 0;
}

