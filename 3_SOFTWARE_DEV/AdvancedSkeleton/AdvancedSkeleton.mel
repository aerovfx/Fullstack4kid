delete cvPos;
delete $tempString[0];
//$pos=`xform -q -ws -t ($ctrl+".cv[0]")`;
$wScale=`xform -q -ws -s $ctrl`;
move -r -os -wd 0 0 ($maxZ*$wScale[0]) ($ctrl+".cv[0:99]");
}

